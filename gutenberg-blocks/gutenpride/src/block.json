{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/gutenpride",
  "version": "0.2.0",
  "title": "Gutenpride",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
	"html": false
  },
  "attributes": {
	"showPostCounts": {
	  "type": "boolean",
	  "default": false
	},
	"selectedCategory": {
	  "type": "number",
	  "default": ""
	},
	"displayAuthor": {
	  "type": "boolean",
	  "default": false
	},
	"displayFutureImg": {
	  "type": "boolean",
	  "default": false
	}
  },
  "textdomain": "gutenpride",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
